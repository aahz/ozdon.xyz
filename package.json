{
  "name": "ozdon.xyz",
  "title": "OZDON",
  "description": "Software developer",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "install:clean": "rm -rf node_modules/* && rm -rf yarn.lock && yarn install && git add ./yarn.lock && git commit --allow-empty -m \"Clean install. Updated yarn.lock file.\"",
    "install:ci": "yarn install --pure-lockfile --non-interactive --no-progress",
    "build": "npm run clean && webpack --config=configs/webpack/prod.js",
    "watch": "npm run clean && webpack --config=configs/webpack/dev.js --watch --progress --color",
    "clean": "rm -rf ./dist/*",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "eslint": "eslint \"./source/**/*.{js,jsx,ts,tsx}\" --quiet",
    "eslint:fix": "eslint --fix \"./source/**/*.{js,jsx,ts,tsx}\" --quiet",
    "stylelint": "stylelint \"source/**/*.{less,css}\"",
    "stylelint:fix": "stylelint --fix \"source/**/*.{less,css}\"",
    "start": "npm run start:dev",
    "start:dev": "npm run clean && webpack --config=configs/webpack/local.js --watch --progress --color",
    "start:prod": "ts-node ./server/index.ts",
    "preversion": "npm run lint && npm run build",
    "version": "git add ./dist/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aahz/ozdon.xyz.git"
  },
  "author": "Andrei Ozdon <andrei@ozdon.xyz> (https://ozdon.xyz/)",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/aahz/ozdon.xyz/issues"
  },
  "homepage": "https://github.com/aahz/ozdon.xyz#readme",
  "dependencies": {
    "normalize.css": "^8.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-private-methods": "^7.14.5",
    "@babel/plugin-proposal-private-property-in-object": "^7.15.4",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@types/node": "^16.10.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.3.0",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.26.1",
    "express": "^4.17.1",
    "favicons": "^6.2.2",
    "favicons-webpack-plugin": "^5.0.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "less": "^4.1.2",
    "less-loader": "^10.0.1",
    "style-loader": "^3.3.0",
    "stylelint": "^13.13.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-order": "^4.1.0",
    "ts-loader": "^9.2.6",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "webpack": "^5.58.1",
    "webpack-cli": "^4.9.0",
    "webpack-merge": "^5.8.0"
  }
}
